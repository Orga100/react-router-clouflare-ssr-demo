{
  "name": "react-router-hono-fullstack-template",
  "description": "A modern full-stack template powered by Cloudflare Workers, using Hono for backend APIs, React Router for frontend routing, and Radix UI components styled with CSS Modules",
  "private": true,
  "type": "module",
  "cloudflare": {
    "label": "React Router + Hono Fullstack App",
    "products": [
      "Workers"
    ],
    "categories": [
      "starter"
    ],
    "docs_url": "https://developers.cloudflare.com/workers",
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/cd71c67a-253f-477d-022c-2f90cb4b3d00/public",
    "icon_urls": [],
    "publish": true
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-form": "^0.1.8",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.2.1",
    "dotenv": "^17.2.2",
    "hono": "4.8.2",
    "isbot": "5.1.26",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "7.6.3",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.7.5",
    "@cloudflare/workers-types": "^4.20250909.0",
    "@react-router/dev": "7.6.3",
    "@types/node": "^24.0.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "cross-env": "^10.0.0",
    "jsonc-parser": "^3.3.1",
    "purgecss": "^7.0.2",
    "swagger-ui-watcher": "^2.1.14",
    "typescript": "5.8.3",
    "vite": "^6.0.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "5.1.4",
    "wrangler": "^4.28.1"
  },
  "scripts": {
    "api": "swagger-ui-watcher docs/swagger.yaml",
    "build": "react-router build && npm run purge-css",
    "cf-typegen": "wrangler types",
    "deploy": "npm run build && wrangler deploy",
    "dev": "cross-env PORT=${PORT:-3000} react-router dev --port ${PORT:-3000}",
    "preview": "npm run build && vite preview",
    "preview_wrangler_local_preview_kv": "wrangler dev",
    "preview_wrangler_remote_preview_kv": "wrangler dev --remote",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "logs": "wrangler tail",
    "import_kv": "node ./scripts/import_kv.js",
    "export_kv": "node ./scripts/export_kv.js",
    "purge-css": "node ./scripts/purge-css.js"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
